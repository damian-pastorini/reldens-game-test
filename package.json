            "source": "./theme/custom-game-theme-test/index.html",
            "distDir": "./dist"
        }
    },
    "dependencies": {
        "@babel/core": "^7.17.8",
        "@reldens/items-system": "^0.13.0",
        "@reldens/utils": "^0.13.0",
        "reldens": "^4.0.0-beta.23"
    },
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.4.0",
        "@parcel/transformer-webmanifest": "^2.4.0",
        "sass": "^1.49.9"
    }
