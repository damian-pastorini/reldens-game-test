    "license": "MIT",
    "main": "index.js",
    "browser": {
        "pg-connection-string": false,
        "crypto": false
    },
    "scripts": {
        "start": "node ."
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "browserslist": ["> 0.5%, last 2 versions, not dead"],
